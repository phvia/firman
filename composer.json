{
    "name": "phvia/firman",
    "description": "PHP multi-process and non-blocking I/O library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "farwish",
            "email": "farwish@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "symfony/console": "~4.0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.7"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Firman\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firman\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "suggest": {
        "php": "Suggest to install newest PHP for high performance."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
